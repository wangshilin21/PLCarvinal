<view class="page">
	<view class="login-wrap">
    <view class="logo-wrap">
		 <image class="logo" src="../../images/logo.png"></image> 
      </view>
		<view class="weui-cells weui-cells_after-title">
		    <view class="weui-cell weui-cell_input">
		        <view class="weui-cell__hd">
		            <view class="weui-label">账号 Account</view>
		        </view>
		        <view class="weui-cell__bd">
		            <input value="{{userName}}" class="weui-input" bindchange="inputUserName" placeholder="输入账号 Input Account"/>
		        </view>
		    </view>
		    <view class="weui-cell weui-cell_input weui-cell_vcode">
		        <view class="weui-cell__hd">
		            <view class="weui-label">密码 Password</view>
		        </view>
		        <view class="weui-cell__bd">
		            <input value="{{userPassword}}" class="weui-input" type="password" bindchange="inputUserPassword" placeholder="输入密码 Input Password" />
		        </view>
		    </view>   
		</view>
		<checkbox-group class="remenber" bindchange="bindAgreeChange">
        </checkbox-group>
    <image class="loginIcon" bindtap="onLogin" src="../../images/loginIcon.png"></image>
    <view>
    <image class="bottomIcon" src="../../images/bottomIcon.png"></image> 
    </view>
	</view>
</view>
