/**index.wxss**/
.page {
	text-align: center;
	height: 100%;
  width: 100%;
  position: relative;

}
.page-wrap {
	position: relative;
  padding-top: 20rpx;
}
.btn button{
	width: 95%;
  height: 250rpx;
	margin-bottom: 10rpx;
  align-items: center;
}
.btn image {
	width: 150rpx;
	height: 150rpx;
	display: inline-block;
	vertical-align: middle;
  margin-top: 10rpx;
}
.picInfo{
  color:black;
  font-size: 32rpx;
}
.logo {
    width: 100%;
    height: 200rpx;
    display: inline-block;
    margin-bottom: 0rpx;
    padding-top: 20rpx;
}
.background-image{
   height : 100%;
    position: absolute;
    width: 100%;
    left: 0px;
    top: 0px;
}
.bottomText{
  font-size: 30rpx;
  color:rgb(37, 39, 180);
}
