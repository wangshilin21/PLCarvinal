.showHongbao {
  position: absolute;
  width: 480rpx;
  height: 680rpx;
  margin-top:200rpx;
  margin-left:125rpx;
  background: rgb(173, 173, 175);
}

.hb_foot {
  position: absolute;
  top: 280rpx;
  width: 480rpx;
  height: 400rpx;
  background: #cd0000;
  border-radius: 0 0 30rpx 30rpx;
  z-index: 3;
}

.hb_head {
  position: absolute;
  top: 120rpx;
  width: 480rpx;
  height: 170rpx;
  background: red;
  z-index: 1;
}

.hb_body {
  position: absolute;
  top: 240rpx;
  width: 480rpx;
  height: 150rpx;
  background: red;
}

.hb_open {
  position: absolute;
  z-index: 5;
  background: #ee0;
  width: 120rpx;
  height: 120rpx;
  border-radius: 50rpx;
  left: 180rpx;
  line-height: 100rpx;
  text-align: center;
  color: #a6a6a6;
}

.hb_money {
  position: absolute;
  left: 50rpx;
  width: 360rpx;
  height: 360rpx;
  background: white;
  z-index: 2;
  text-align: center;
  line-height: 100rpx;
}

.avoidDrinkOnce {
  width: 100%;
  height: 100%;
}
.backToMain{
  display: flex;
  padding-top: 60rpx;
  justify-content: center;
  align-content: center;
}
.backToMain button {
  width: 300rpx;
  height: 80rpx;
  font-size: 36rpx;
  line-height: 80rpx;
  border-radius: 80rpx;
  /*color: #fff;*/
  color: #1e5adb;
  background: #fff;
  border: solid 1rpx #2947f1;
}